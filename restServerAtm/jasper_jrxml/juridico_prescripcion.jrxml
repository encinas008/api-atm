	<subDataset name="Dataset1" uuid="8cae55eb-d333-4133-9601-a357fd5f40ec">
		<property name="com.jaspersoft.studio.data.defaultdataadapter" value="PostgreSQL"/>
		<parameter name="id" class="java.lang.String"/>
		<queryString language="SQL">
			<![CDATA[select pot.gestion
from pres_prescripcion as pp
left join pres_representante_legal as prl on prl.id_prescripcion = pp.id
join pres_objeto_tributario as pot on pot.id_prescripcion = pp.id
where pp.id=  $P!{id}]]>
		</queryString>
		<field name="gestion" class="java.lang.Integer"/>
	</subDataset>
	<parameter name="id" class="java.lang.String">
		<parameterDescription><![CDATA[]]></parameterDescription>
	</parameter>
	<queryString language="SQL">
		<![CDATA[select p.*,prl.*, d.direccion, d.telefono, d.celular, d.zona, ptot.name, pot.numero, pp.fur
from pres_prescripcion as pp
join pres_representante_legal as prl on prl.id_prescripcion = pp.id 
join pres_objeto_tributario as pot on pot.id_prescripcion = pp.id
 join pres_tipo_objeto_tributario as ptot on ptot.id = pot.id_tipo_objeto_tributario
left join persona as p on p.id_prescripcion = pp.id 
left join domicilio as d on d.id_persona = p.id 
where pp.id =  $P!{id}  
group by p.id, prl.id, d.direccion, d.telefono, d.celular, d.zona, ptot.name, pot.numero, pp.fur]]>
	</queryString>
